{
    "name": "bccampus/wp-bootwalker",
    "description": "WordPress Nav Walkers to implement Bootstrap navbars.",
    "homepage": "https://github.com/BCcampus/wp-bootstrap-nav-walker",
    "version": "1.0.0",
    "keywords": ["wordpress", "bootstrap", "bootstrap 4", "submenu", "nav walker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brad Payne",
            "email": "brad@bradpayne.ca"
        }
    ],
    "require-dev": {
        "humanmade/coding-standards": "^0.2.1"
    },
    "autoload": {
        "psr-4": {
            "BCcampus\\": "inc"
        }
    },
    "scripts": {
        "test": [
            "@standards"
